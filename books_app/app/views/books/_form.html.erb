<%= form_with model: @book, local: true do |form| %>
 
 
  <p>
    <%= form.label :title %><br>
    <%= form.text_field :title %>
  </p>
 
  <p>
    <%= form.label :author %><br>
    <%= form.text_field :author %>
  </p>

  <p>
    <%= form.label :description %><br>
    <%= form.text_field :description %>
  </p>
 
  <p>
  <%= form.label :url %><br>
  <%= form.text_field :url %>
  </p>

  <p>
    <%= form.submit %>
  </p>
 
  <% end %>
